<Window x:Class="ETS_TOOL.UpdateMoneyModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETS_TOOL"
        mc:Ignorable="d"
        Title="UpdateMoneyModal" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="NewMoneyTextBox" HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="UpdateMoneyButton" Content="Update" HorizontalAlignment="Left" Margin="150,39,0,0" VerticalAlignment="Top" Width="119" Click="UpdateMoneyButton_Click"/>

    </Grid>
</Window>
